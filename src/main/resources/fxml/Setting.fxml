<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="280.0" prefWidth="380.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="mobi.huanyuan.spider.ui.controller.SettingController">
    <Label alignment="CENTER" text="配置" AnchorPane.leftAnchor="147.0" AnchorPane.topAnchor="10.0">
        <font>
            <Font size="24.0"/>
        </font>
    </Label>
    <GridPane layoutX="-16.0" layoutY="31.0" prefHeight="100.0" prefWidth="360.0" AnchorPane.leftAnchor="10.0"
              AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="80.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label text="本地存储路径" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
        <TextField fx:id="localPath" editable="false" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
    </GridPane>
    <Button mnemonicParsing="false" onAction="#saveSettings" prefWidth="90.0" text="保存"
            AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="50.0"/>
</AnchorPane>
